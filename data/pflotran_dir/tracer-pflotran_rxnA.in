CHEMISTRY
PRIMARY_SPECIES
  Tracer1
  Tracer2

/


GENERAL_REACTION
  REACTION Tracer1 <-> Tracer2
  FORWARD_RATE 5e-5 ! 1/s

/


SORPTION
  ISOTHERM_REACTIONS
    Tracer1
     TYPE LINEAR
     DISTRIBUTION_COEFFICIENT 0 ! kg water/m^3 bulk
     /
     Tracer2
     TYPE LINEAR
     DISTRIBUTION_COEFFICIENT 0 ! kg water/m^3 bulk
     /
   
   /
  /



LOG_FORMULATION
DATABASE db/hanford.dat
END

CONSTRAINT rainfall
  CONCENTRATIONS
    Tracer1 1.0 T
    Tracer2 1.0e-20 T

  /
END

CONSTRAINT initial
  CONCENTRATIONS
    Tracer1 1.0e-20 T
    Tracer2 1.0e-20 T

  /
END

